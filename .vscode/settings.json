{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.tabSize": 2,
  "editor.rulers": [90],
  "editor.codeActionsOnSave": { "source.fixAll": true },
  "editor.showFoldingControls": "always",
  "editor.folding": true,
  "editor.foldingStrategy": "indentation",
  "eslint.alwaysShowStatus": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": "all",
  "typescript.inlayHints.variableTypes.enabled": "all",
  "typescript.inlayHints.propertyDeclarationTypes.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": "all",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.tsdk": "node_modules/typescript/lib",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.defaultProfile.windows": "Node.js",
  "terminal.integrated.profiles.windows": {
    "Node.js": {
      "path": "C:\\Program Files\\nodejs\\node.exe",
      "args": []
    }
  }
}
